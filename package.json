    "expo-notifications": "~0.28.9",
    "expo-splash-screen": "^0.27.5",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.6",
    "geolib": "^3.3.4",
    "i18n-js": "3.9.2",
    "lodash": "^4.17.21",
    "lucide-react-native": "^0.376.0",
    "mobx": "^6.11.0",
    "mobx-react-lite": "^4.0.5",
    "mobx-state-tree": "^5.3.0",
    "react": "18.2.0",
    "react-native": "0.74.2",
